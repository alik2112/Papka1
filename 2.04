#include <iostream>

int main(){
    for (int hundred = 1; hundred < 10; ++hundred){
        for (int ten = 0; ten < 10; ++ten){
            for (int one = 0; one < 10; ++one){
                int armstrong = 100 * hundred + 10 * ten + one;
                if (hundred * hundred * hundred + ten * ten * ten + one * one * one == armstrong){
                    std::cout << armstrong << " ";
                }
            }
        }
    }
}
